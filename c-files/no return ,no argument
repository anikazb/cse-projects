//no return ,no argument

#include<stdio.h>
void sum()
{ int n,i,fact=1;
printf("enter the number=");
scanf("%d",&n);
for(i=1;i<=n;i++)
{fact=fact*i;
printf("the factorial of given number=%d\n",fact);}
}
int main()
{ sum();
return 0;}
