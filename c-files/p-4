//odd num pattern
#include<stdio.h>
int main()
{ int column,row;
printf("row=");
scanf("%d",&row);
printf("column=");
scanf("%d",&column);

for(int i=1;i<=column;i++)
{int a=1;
    for(int j=1;j<=row;j++)
 {printf("%d ",a);
     a=a+2;
 }
 printf(" \n");}
 return 0;}
/* 
 row=5
column=5
1 3 5 7 9  
1 3 5 7 9  
1 3 5 7 9  
1 3 5 7 9  
1 3 5 7 9  
*/
