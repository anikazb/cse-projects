// Online C compiler to run C program online
#include <stdio.h>
#include<math.h>
int sum(int n) 
{ int flag=1,i;
double sqroot=sqrt(n);
for(i=2;i<n;i++)
{ if( n%i==0)
flag=0; break;}
{
if(flag)
     printf("prime=%d\n",n);
else
    printf("not prime=%d\n",n);
    
}

    return 0;
}
void result(int start,int end)
{ int i;
for(i=start;i<=end;i++)
 { if(sum(i))
 printf("%d",i);}
}
int main(void)
{ int start,end;
printf("the starting limit and the ending limit=\n");
scanf("%d %d",&start,&end);
result(start,end);
return 0;
}

